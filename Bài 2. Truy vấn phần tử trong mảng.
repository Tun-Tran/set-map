#include <bits/stdc++.h>
using namespace std;


int main() {
    int n;cin>>n;
    set<int> s;
    for(int i=0;i<n;i++){
        int a;cin>>a;
        s.insert(a);
    }
    int b;cin>>b;
    
    for(int i=0;i<b;i++){
        int tmp;cin>>tmp;
        int k;k =tmp;
        if(s.find(k)== s.end()){
            cout<<"NO"<<endl;
        }
        else cout<<"YES"<<endl;
    }
    return 0;
}
